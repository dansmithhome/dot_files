function shunt-logs
{

    local log4j_properties=${p}/java/res/log4j.properties
    local tmp_file=/tmp/properties-$$
    
    if [[ ! -f ${log4j_properties} ]]
    then
        echo Cannot find properties file: ${log4j_properties}  1>&2 
        exit 1
    fi

    cp ${log4j_properties} ${tmp_file}  || exit 1

    sed < ${tmp_file} > ${log4j_properties} \
        -e 's/log4j.logger.com.perkinelmer.connector=.*/log4j.logger.com.perkinelmer.connector=WARN/' \
        -e 's/log4j.logger.com.perkinelmer.data=.*/log4j.logger.com.perkinelmer.data=WARN/' \
        -e 's/log4j.logger.com.perkinelmer.indexing=.*/log4j.logger.com.perkinelmer.indexing=WARN/' \
        -e 's/log4j.logger.com.perkinelmer.model=.*/log4j.logger.com.perkinelmer.model=WARN/' \
        -e 's/log4j.logger.com.perkinelmer.security=.*/log4j.logger.com.perkinelmer.security=WARN/' 


    rm -f ${tmp_file}
}
    
    
